<Window x:Class="PrismWpfExample.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <Grid>
		<Label Content="{Binding DateString}" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" Width="175"/>
		<Button Command="{Binding ShowDateStringCommand}" Content="Button" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Margin="0,150,0,0" VerticalAlignment="Center" Width="175"/>
		<!-- <ContentControl prism:RegionManager.RegionName="ContentRegion" /> -->

    </Grid>
</Window>
